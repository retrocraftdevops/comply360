{
  "name": "comply360-mobile",
  "version": "1.0.0",
  "description": "Comply360 Mobile App - SADC Corporate Gateway Platform",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "clean": "rm -rf node_modules && rm -rf ios/Pods && rm -rf ios/build && rm -rf android/build",
    "pod-install": "cd ios && pod install && cd ..",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace Comply360.xcworkspace -scheme Comply360 -configuration Release"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.1",
    "react-native-paper": "^5.12.1",
    "react-native-vector-icons": "^10.0.3",
    "axios": "^1.6.5",
    "react-native-biometrics": "^3.0.1",
    "react-native-camera": "^4.2.1",
    "react-native-document-scanner-plugin": "^0.4.2",
    "react-native-push-notification": "^8.1.1",
    "@react-native-community/geolocation": "^3.2.1",
    "react-native-image-picker": "^7.1.0",
    "react-native-permissions": "^4.1.3",
    "react-native-keychain": "^8.1.2",
    "react-native-device-info": "^10.13.1",
    "react-native-config": "^1.5.1",
    "date-fns": "^3.0.6",
    "yup": "^1.3.3",
    "formik": "^2.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.23.7",
    "@react-native/babel-preset": "^0.73.19",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.48",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "@testing-library/react-native": "^12.4.3",
    "react-test-renderer": "18.2.0",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
